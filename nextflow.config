process.container = 'library://vchivukula/collection/elgato:latest' 

profiles {
  singularity {        
    singularity.enabled=true        
    singularity.autoMounts = true
    singularity.cacheDir = "$HOME/cache/"
  }

  conda {
    //Setting the correct conda environments
    conda.enabled = true
    process.conda = "appliedbinf::elgato"
  }
}


